public class toSeconds 
{
    //
    //adds together hours, minutes, and seconds
    //by changing them all into seconds
    //
    public static void main(String args[])
    {
    //variable decorations
      int hrs = 2;
      int mins = 5;
      int sec = 13;
      int result;
      System.out.println("Hrs: " + hrs + " Mins: " + mins + " Secs: " + sec);
     //Computation
      hrs = hrs * 3600;
      mins = mins * 60;
      result = hrs + mins + sec;
     //prints out answer
     System.out.println("Total Seconds:" + result);
      
    }//end of main
}//end of toSeconds
