public class avgOfThree
{
    //prints out the average of three integers
    public static void main(String args[])
    {
    //var assignments
      int x = 10;
      int y = 25;
      int z = 16;
      int avg = (x + y + z)/3;
    //prints out solution
      System.out.println("Average of x, y and z: " + avg);
    }//end of main
}//end of avgOfThree
