public class tempConverter
{
    //changes a fahrenhiet value to a celcius temp value
    //formula C = (F-32) * 5/9
    public static void main(String args [])
    {
        //constants
        final int BASE = 32;
        final double CONVERSION_FACTOR = 5.0/9.0;
        //variables
        int fahrenheitTemp = 90; // value to covert
        double celciusTemp;
        //computation
       celciusTemp = (fahrenheitTemp - BASE) * CONVERSION_FACTOR;
        //prints results
        System.out.println("Fahrenheit Temperature: " + fahrenheitTemp);
        System.out.println("Celcius Tempurature: "+ celciusTemp);
    }//end of main
}//end of tempConverter
